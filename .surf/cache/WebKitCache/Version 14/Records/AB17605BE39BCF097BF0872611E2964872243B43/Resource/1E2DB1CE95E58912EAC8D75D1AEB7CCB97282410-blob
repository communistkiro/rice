		<script>
			const CFG = {}
			var st = localStorage.getItem('store') || '{}';
			st = JSON.parse(st);
			tmp_styles = {
		        makaba: false,
		        makabaverdana: '/makaba/templates/css/makabaverdana.css',
		        makabacentered: '/makaba/templates/css/makabacentered.css',
		        futaba: '/makaba/templates/css/futaba_.css',
		        burichan: '/makaba/templates/css/burichan_.css',
		        muon: '/makaba/templates/css/muon_.css',
		        neutron: '/makaba/templates/css/neutron_.css',
		        gurochan: '/makaba/templates/css/gurochan_.css',
		        konsole: '/makaba/templates/css/konsole_.css',
		    };
			var customStyle = st.hasOwnProperty('styling') ? st.styling.style : false; 
		    if(customStyle && tmp_styles[customStyle]) {
			    document.write('<link href="' + tmp_styles[customStyle] + '" id="dynamic-style-link" type="text/css" rel="stylesheet">');
		    }
		    var customCss = st.hasOwnProperty('other') ? st.other.custom_css : {};
		    var style = document.createElement('style');
		    if(customCss && customCss.hasOwnProperty('enabled') && customCss.hasOwnProperty('data')) {
		    	if(style.styleSheet){
				    style.styleSheet.cssText = customCss.data;
				}else{
				    style.appendChild(document.createTextNode(customCss.data));
				}
				document.getElementsByTagName('head')[0].appendChild(style);
		    }
		    var tmp_boards;
		    try { tmp_boards = st.other.myboards.boards;} catch(e) { tmp_boards = ['b','news']; }
		    
		    var renderBoards = function(boards) {
		        var tmp_html = '';
		        tmp_html += '[ ';
		        for(var i = 0; i < boards.length; i++) {
		            tmp_html += ' <a href="/' + boards[i] + '/">' + boards[i] + '</a> ';
		            if(i < boards.length - 1) tmp_html+= '/ ';
		        }
		        tmp_html += '/ <a title="Настроить список" id="edit-boards" href="#">+</a>';
		        tmp_html += ' ]';
		        document.getElementsByClassName('adminbar__cat_type_myboards')[0].innerHTML = tmp_html;
		    }
		    let nightmode = st.hasOwnProperty('styling') && st.styling.nightmode ? st.styling.nightmode : false;
		    CFG.NIGHTMODE = nightmode;
			if ( nightmode ) { 
				document.documentElement.dataset.theme = 'nightmode'; 
			}
			//@todo scope
		</script>
	</head>
