return false;}};OO.ui.TextInputWidget.prototype.onKeyPress=function(e){if(e.which===OO.ui.Keys.ENTER){this.emit('enter',e);}};OO.ui.TextInputWidget.prototype.onBlur=function(){this.setValidityFlag();};OO.ui.TextInputWidget.prototype.onFocus=function(){if(this.isWaitingToBeAttached){this.onElementAttach();}this.setValidityFlag(true);};OO.ui.TextInputWidget.prototype.onElementAttach=function(){this.isWaitingToBeAttached=!1;this.valCache=null;this.positionLabel();};OO.ui.TextInputWidget.prototype.onDebouncedChange=function(){this.setValidityFlag();};OO.ui.TextInputWidget.prototype.isReadOnly=function(){return this.readOnly;};OO.ui.TextInputWidget.prototype.setReadOnly=function(state){this.readOnly=!!state;this.$input.prop('readOnly',this.readOnly);return this;};OO.ui.TextInputWidget.prototype.isRequired=function(){return this.required;};OO.ui.TextInputWidget.prototype.setRequired=function(state){this.required=!!state;if(this.required){this.$input.prop('required',true).attr(
