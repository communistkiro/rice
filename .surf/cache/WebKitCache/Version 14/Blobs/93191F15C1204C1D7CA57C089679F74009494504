		 <script>

		function parseFlt(str, radix)
		{
        var parts = str.split(".");
        if ( parts.length > 1 )
        	{
                return parseInt(parts[0], radix) + parseInt(parts[1], radix) / Math.pow(radix, parts[1].length);
        	}
        return parseInt(parts[0], radix);
		}


		function HexToDec(){
			var HexValue = document.getElementById("cctextinputHex").value

			var HexValue2 = HexValue.toString(16);
			var DecValue = parseFlt(HexValue2, 16);
			document.getElementById("cctextinputDec").value = DecValue;

			document.getElementById("ccOutput5").innerHTML = HexValue+" in Hexadecimal is equal to "+DecValue+" in Decimal";

		}

		function DecToHex(){
			var DecValue = document.getElementById("cctextinputDec").value
			var DecValue2 = parseFloat(DecValue); 
			var hexString = DecValue2.toString(16);
			document.getElementById("cctextinputHex").value = hexString;

			document.getElementById("ccOutput5").innerHTML = hexString+" in Hexadecimal is equal to "+DecValue+" in Decimal";
		}


		function ccCalculatefn5() {
			var HexValue = document.getElementById("cctextinputHex").value

			var HexValue2 = HexValue.toString(16);
			var DecValue = parseFlt(HexValue2, 16);
			document.getElementById("cctextinputDec").value = DecValue;

			document.getElementById("ccOutput5").innerHTML = HexValue+" in Hexadecimal is equal to "+DecValue+" in Decimal";
		}
		 </script></div>
